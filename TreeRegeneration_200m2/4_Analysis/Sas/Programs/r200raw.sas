FILENAME TREE92o 'F:\TI-REGEN\twohunM2\200m2reg.92D';
FILENAME TREE92n 'F:\TI-REGEN\twohunM2\reg200.92D';
DATA QUADIN ;
COMMENT * THIS PROGRAM READS RAW 200m2 DATA AND PUTS THE QUAD NUMBER
 AT THE BEGINNING OF EACH LINE;
INFILE tree92o dlm=',';
*LENGTH LINE $ 37;
LENGTH QUAD $ 3 test $ 3;
INPUT LINE $char37.;
test=SUBSTR(LINE,4,3);
if test='   ' then do;
  QUAD=SUBSTR(LINE,1,3);
  RETAIN QUAD;
  DROP LINE;
  RETURN;
END;
ELSE DO;
FILE tree92n;
PUT QUAD 1-3 LINE 5-41;
END;
RUN;
